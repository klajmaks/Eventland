<div id="pins" class="transitions-enabled">
  <% @pins.each do |pin| %>
    <div class="box">
      <div class="panel panel-default">
        <div class="panel-body">
        <%= link_to image_tag(pin.image.url(:medium)), pin %><br/>
        <%= pin.description %><br/>
      </div>
        <% if pin.user == current_user %>
        <div class="panel-footer">
        <%= link_to 'Edytuj', edit_pin_path(pin) %><br/>
        <%= link_to 'Usuń', pin, method: :delete, data: { confirm: 'Jesteś pewny?' } %><br/>
      </div>
        <% end %>
      </div>
    </div>
    <% end %>
</div>
<%= will_paginate @posts %>
